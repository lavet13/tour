# Required
CERTBOT_EMAIL=lavet13@mail.ru

# Optional (Defaults)
DHPARAM_SIZE=2048
ELLIPTIC_CURVE=secp256r1
RENEWAL_INTERVAL=8d
RSA_KEY_SIZE=2048
STAGING=1
USE_ECDSA=1

# Advanced (Defaults)
CERTBOT_AUTHENTICATOR=dns-regru
CERTBOT_DNS_PROPAGATION_SECONDS=1800
DEBUG=1
USE_LOCAL_CA=0

# Reg.ru API credentials - will be used by setup-credentials.sh
REGRU_USERNAME=your_username
REGRU_PASSWORD=your_password
